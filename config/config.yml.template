version: 0.1

log:
    level: info

storage:
    filesystem:
        rootdirectory: /data
    delete:
        enabled: true

http:
    addr: :5000
    headers:
        X-Content-Type-Options: [nosniff]

proxy:
    remoteurl: https://registry.${DOMAIN}

health:
    storagedriver:
        enabled: true
        interval: 10s
        threshold: 3
